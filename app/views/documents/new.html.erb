<h1>New document</h1>

 <% form_for :create, :url => {:action=>:create}, :html=>{:multipart=>true} do |f| %>
<p>
  Document Name:
  <%= f.text_field :name %>
</p>
<p>
	Document<br />
 	<%= f.file_field :file %>
</p>
<p>
  <%= submit_tag 'Create' %>
</p>
<% end %>

<%= link_to 'Back', documents_path %>